prometheus_bearer_token=$Q_LEARN_BEARER_TOKEN
repository_name=$Q_LEARN_GIT_REPO
git_repository_path=/git/$Q_LEARN_GIT_REPO.git
username = $Q_LEARN_USERNAME
password = $Q_LEARN_PASSWORD
